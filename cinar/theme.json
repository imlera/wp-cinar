{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 1,
  "settings": {
    "appearanceTools": true,
    "useRootPaddingAwareAlignments": true,
    "shadow": {
      "defaultPresets": true,
      "presets": [
        {
          "name": "Карточка автомобиля",
          "slug": "car-card",
          "shadow": "0px 4px 20px 0px #33333333"
        }
      ]
    },
    "color": {
      "defaultDuotone": false,
      "defaultPalette": false,
      "defaultGradients": false,
      "duotone": [],
      "gradients": [],
      "palette": [
        {
          "color": "#FFF",
          "name": "White",
          "slug": "white"
        },
        {
          "color": "#EF233C",
          "name": "Imperial Red",
          "slug": "red"
        },
        {
          "color": "#333333",
          "name": "Dark Charcoal",
          "slug": "black"
        },
        {
          "color": "#484848",
          "name": "Outer Space",
          "slug": "gray"
        },
        {
          "color": "#828282",
          "name": "Old Silver",
          "slug": "silver"
        }
      ]
    },
    "layout": {
      "wideSize": "62.5rem",
      "contentSize": "81.25rem"
    },
    "spacing": {
      "spacingScale": {
        "steps": 0
      },
      "spacingSizes": [],
      "units": [
        "%",
        "px",
        "em",
        "rem",
        "vh",
        "vw"
      ]
    },
    "typography": {
      "fluid": true,
      "writingMode": true,
      "fontFamilies": [
        {
          "name": "System Serif",
          "slug": "system-serif",
          "fontFamily": "Iowan Old Style, Apple Garamond, Baskerville, Times New Roman, Droid Serif, Times, Source Serif Pro, serif, Apple Color Emoji, Segoe UI Emoji, Segoe UI Symbol"
        },
        {
          "name": "System Sans-serif",
          "slug": "system-sans-serif",
          "fontFamily": "-apple-system, BlinkMacSystemFont, avenir next, avenir, segoe ui, helvetica neue, helvetica, Cantarell, Ubuntu, roboto, noto, arial, sans-serif"
        },
        {
          "name": "Next Art",
          "slug": "next-art",
          "fontFamily": "\"Next Art\", sans-serif",
          "fontFace": [
            {
              "fontFamily": "Next Art",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "fontStretch": "normal",
              "fontWeight": "100 400",
              "src": [
                "file:./assets/fonts/NextArt/NextArt_Regular.otf"
              ]
            },
            {
              "fontFamily": "Next Art",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "fontStretch": "normal",
              "fontWeight": "500 600",
              "src": [
                "file:./assets/fonts/NextArt/NextArt_Bold.otf"
              ]
            },
            {
              "fontFamily": "Next Art",
              "fontStyle": "normal",
              "fontDisplay": "swap",
              "fontStretch": "normal",
              "fontWeight": "700 900",
              "src": [
                "file:./assets/fonts/NextArt/NextArt_Heavy.otf"
              ]
            }
          ]
        }
      ],
      "fontSizes": []
    }
  },
  "styles": {},
  "templateParts": [],
  "customTemplates": []
}
